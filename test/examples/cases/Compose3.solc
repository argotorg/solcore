contract Compose {
  function compose(f,g) {
    return lam (x) {
      return f(g(x));
    } ;
  }

  function id(x) { return x; }

  function idid() { return compose(id,id); }

  function apply1(f, a) { return f(a); }

  function main() {
    let f = compose(id, id);
    let g = f();
    return apply1(g, 42);
  }
}
