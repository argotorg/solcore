function add_(x : word, y : word) {
  let res: word;
  assembly {
     res := add(x, y)
  }
  return res;
}

contract SimpleLambda{
  function f (z) {
    let n = lam (x,y) {
      return add_(x, add_(y,1));
    } ;
    let m = lam (x) {
      return add_(z,x) ;
    } ;
    return m(n(1,0));
  }
  function main() {
    return f(40);
  }
}
