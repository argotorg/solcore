function addWord(x : word, y : word) -> word {
  let res: word;
  assembly {
     res := add(x, y)
  }
  return res;
}

contract SimpleLambda{
  function f (z) {
    let n = lam (x,y) {
      return addWord(x,addWord(y,1));
    } ;
    let m = lam (x) {
      return addWord (z,x) ;
    } ;
    return m(n(1,0));
  }
  function main() {
    return f(50);
  }
}
