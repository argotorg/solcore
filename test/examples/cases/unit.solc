contract Unit {
  function one (x : ()) -> word {
    return 1;
  }

  function unitVal() -> () {
    return ();
  }

  function unitMatch (x : ()) -> word {
    match x {
    | () => return 1;
    }
  }

  function foo (x : word) -> () {
    return ();
  }

  function main() -> word {
    return unitMatch(foo(one(unitVal())));
  }
}

forall a . class a : Def {
  function def () -> a ;
}

instance () : Def {
  function def() -> () {
    return ();
  }
}
