data Bool = True | False; 

class a : Eq {
  function eq (x : a, y : a) -> Bool;
}

forall a . a : Eq => class a : Ord {
  function lt (x : a, y : a) -> Bool ; 
}

instance word : Eq {
  function eq (x,y) {
    match primEqWord(x,y) {
    | 0 => 
      return False; 
    | _ => 
      return True ;
    };
  }
}
