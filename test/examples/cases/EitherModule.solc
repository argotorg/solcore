contract EitherModule {
  data Either(a,b) = Left(a) | Right(b);
  data List(a) = Nil | Cons(a,List(a));

  function lefts(xs) {
    match xs {
    | Nil => return Nil ;
    | Cons(y,ys) => 
      match y {
      | Left(z) => return Cons(z,lefts(ys)) ; 
      | Right(z) => return lefts(ys) ;
      };
    };
  }

}
