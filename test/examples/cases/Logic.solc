contract Logic {
  data Bool = True | False 

  function not (x) {
    match x {
    | True => return False ; 
    | False => return True ; 
    };
  }

  function and(x, y) {
    match x, y {
    | False, _ => return False ; 
    | True , _ => return y ; 
    };
  }

  function and1 (x, y) {
    match x, y {
    | False, False => return False ; 
    | True , False => return False; 
    | False ,True => return False; 
    | True, True => return True;
    };
  }

  function elim (f, g, x) {
    match x {
    | True => return f(x);
    | False => return g(x);
    };
  }
}
