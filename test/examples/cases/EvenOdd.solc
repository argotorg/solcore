contract EvenOdd {
  data Nat = Zero | Succ(Nat);
  data Bool = False | True;

  function even (n : Nat) -> Bool {
    match n {
    | Zero => return True;
    | Succ(m) => return odd(m);
    }
  }

  function odd(n : Nat) -> Bool {
    match n {
    | Zero => return False;
    | Succ(m) => return even(m);
    }
  }
}
