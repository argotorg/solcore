contract Not {
  data Bool = False | True;

  function main() -> word {
    return fromBool(bnot(False));
  }

  function fromBool(b : Bool) -> word {
    match(b) {
      | False => return 0;
      | True  => return 1;
    }
  }

  function bnot(b : Bool) -> Bool {
    match b {
      | False => return True;
      | True => return False;
    }
  }
}
