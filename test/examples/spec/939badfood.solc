class a: Enum {
  function fromEnum(x : a) -> word;
}

data Food = Curry | Beans | Other;

instance Food : Enum {
  function fromEnum(x : Food) {
     match x {
       | Curry => return 1;
       | Beans => return 2;
       | Other => return 3;
     }
  }
}

contract Food {
  function main() {
        return Enum.fromEnum(Beans);
  }
}
