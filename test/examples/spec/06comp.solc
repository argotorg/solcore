contract Compose {
  forall a b c . function compose(f : (b) -> c,g : (a) -> b) -> ((a) -> c) {
    return lam (x) {
      return f(g(x));
    } ;
  }

  forall a . function id(x : a) -> a { return x; }

  forall a . function idid() -> ((a) -> a){ return compose(id,id); }

  function foo() -> word {
    let f = idid();
    return f(42);
  }

  function main() -> word {
    let f = compose(id,id);
    return f(42);
  }
}
