contract Compose {
  function compose(f,g) {
    return lam (x) {
      return f(g(x));
    } ;   
  }

  function id(x) { return x; }

  function idid() { return compose(id,id); }

  function foo() { 
    let f = idid(); 
    let g = f();
    return g(42); 
  }

  function main() {
    let f = compose(id,id);
    let g = f();
    return g(42);
  }
}
