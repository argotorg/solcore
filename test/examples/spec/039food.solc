
data Food = Curry | Beans | Other;
data CFood = Red(Food) | Green(Food) | Nocolor;




  function fromEnum(x : Food) {
     match x {
       | Curry => return 1;
       | Beans => return 42;
       | Other => return 3;
     }
  }


contract Food {
  function eat(x : CFood) -> Food {
    match x {
       | Red(f) => return f;
       | Green(f) => return f;
       | _ => return Other;
    }
  }

  function main() -> word {
  return fromEnum(eat(Green(Beans)));
  }
}
