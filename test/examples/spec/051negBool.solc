forall a.
class  a : Neg {
   function neg(x:a) -> a;
}

data B = F | T;


instance B : Neg {
  function neg (x : B) -> B {
    match x {
    | F => return T;
    | T => return F;
    }
  }
}


contract NegBool {

  function fromB(b:B) -> word {
    match b  {
    | F => return 0;
    | T => return 1;
    }
  }

  function main() -> word { return  fromB(Neg.neg(F)); }
}
