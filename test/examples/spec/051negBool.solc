
class  a : Neg {
   function neg(x:a) -> a;
}

data B = F | T;


instance B : Neg {
  function neg (x : B) {
    match x {
    | F => return T;
    | T => return F;
    };
  }
}


contract NegBool {

  function fromB(b) {
    match b  {
    | F => return 0;
    | T => return 1;
    };
  }

  function main() { return  fromB(Neg.neg(F)); }
}
