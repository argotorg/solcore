import dispatch;
contract Counter {
  // some dummy fields to test offset calculation
  fld0 : word;
  fld1 : uint256;
  fld2 : address;
  counter : word;
  
  constructor() {
    counter = 41;
    fld2 = address(0);
    fld1 = uint256(11);
    fld0 = 7;
  }
  
  function main() -> word {
    counter = counter + 1;
    return counter;
  }
}
