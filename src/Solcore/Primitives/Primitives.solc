  data Unit = Unit 
  type Memory[a] = Word

  class ref : Ref[deref] {
    function load (r : ref) -> deref ; 
    function store (r : ref, v : deref) ;
  }

  type Stack [a] = a 

  instance Stack[a] : Ref [Memory[a]] {
    function load (r : Stack[a]) -> Memory[a] {}

    function store(r : Stack[a], v : Memory[a]) {}
  }
